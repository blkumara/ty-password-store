<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>password</servlet-name>
		<servlet-class>com.ty.Controller.UserController</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>hiemployee</servlet-name>
		<servlet-class>com.ty.Controller.EmployeeController</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>hibank</servlet-name>
		<servlet-class>com.ty.Controller.BankController</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>hiuserretriver</servlet-name>
		<servlet-class>com.ty.Controller.RetriveUserServlet</servlet-class>
	</servlet>
	
	
	
	
	<servlet-mapping>
		<servlet-name>password</servlet-name>
		<url-pattern>
			/password
		</url-pattern>
	</servlet-mapping>	

	<servlet-mapping>
		<servlet-name>hiemployee</servlet-name>
		<url-pattern>
			/employee
		</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>hibank</servlet-name>
		<url-pattern>
			/bank
		</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>hiuserretriver</servlet-name>
		<url-pattern>
			/userretrive
		</url-pattern>
	</servlet-mapping>
	
</web-app>
